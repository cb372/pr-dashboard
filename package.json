{
  "name": "pr-dashboard",
  "version": "0.0.1",
  "description": "PR Dashboard - see all your open PRs in one place",
  "dependencies": {
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-object-assign": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "json-loader": "^0.5.4",
    "node-sass": "^3.3.3",
    "oauthio-web": "^0.5.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "cp ./src/index.html ./dist/index.html; webpack ./src/js/app.js ./dist/js/app.js --config ./webpack.conf.js",
    "start": "cp ./src/index.html ./dist/index.html; webpack-dev-server --config ./webpack.conf.js --content-base ./dist/",
    "clean": "rm -r ./dist"
  }
}
